{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "extends": ["tslint-plugin-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": {
      "severity": "warning",
      "options": {
        "parser": "typescript",
        "trailingComma": "es5"
      }
    }
  }
}
